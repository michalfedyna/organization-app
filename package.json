{
  "name": "OrganizationApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "android": "react-native run-android --no-packager",
    "ios:simulator": "react-native run-ios --no-packager --simulator \"iPhone 14 Pro\"",
    "lint": "eslint . --fix",
    "lint:tsc": "bash -c tsc-files --noEmit",
    "format": "prettier --write",
    "test": "jest --verbose",
    "test:update": "jest --updateSnapshot",
    "test:perf": "reassure",
    "test:perf:baseline": "reassure --baseline",
    "test:e2e:build:ios:debug": "detox build --configuration ios.sim.debug",
    "test:e2e:build:ios:release": "detox build --configuration ios.sim.release",
    "test:e2e:build:android:debug": "detox build --configuration android.emu.debug",
    "test:e2e:build:android:release": "detox build --configuration android.emu.release",
    "test:e2e:ios:debug": "detox test --configuration ios.sim.debug --take-screenshots all --artifacts-location e2e/artifacts/ios",
    "test:e2e:ios:release": "detox test --configuration ios.sim.release --take-screenshots all --artifacts-location e2e/artifacts/ios",
    "test:e2e:android:debug": "detox test --configuration android.emu.debug --take-screenshots all --artifacts-location e2e/artifacts/android",
    "test:e2e:android:release": "detox test --configuration android.emu.release --take-screenshots all --artifacts-location e2e/artifacts/android",
    "ci:test": "jest --ci",
    "ci:stability": "reassure check-stability",
    "ci:danger": "danger ci",
    "ci:lint": "eslint .",
    "ci:tsc": "tsc --noEmit",
    "staged": "lint-staged",
    "install:all": "npm install;cd ios;bundle install;bundle exec pod install",
    "clean:all": "watchman watch-del-all;rm -rf yarn.lock package-lock.json node_modules android/app/build ios/Pods ios/Podfile.lock ~/Library/Developer/Xcode/DerivedData",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-navigation/drawer": "6.6.4",
    "@react-navigation/native": "6.1.8",
    "@react-navigation/native-stack": "6.9.14",
    "@reduxjs/toolkit": "1.9.7",
    "react": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-hook-form": "7.47.0",
    "react-native": "0.72.5",
    "react-native-gesture-handler": "2.13.1",
    "react-native-reanimated": "3.5.4",
    "react-native-safe-area-context": "4.7.2",
    "react-native-screens": "3.25.0",
    "react-native-svg": "13.14.0",
    "react-redux": "8.1.3",
    "redux": "4.2.1",
    "styled-components": "6.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@jest/globals": "^29.7.0",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.3.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/detox": "^18.1.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.28",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "danger": "^11.3.0",
    "detox": "^20.13.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "lint-staged": "^14.0.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "^18.2.0",
    "reassure": "^0.9.1",
    "ts-jest": "^29.1.1",
    "tsc-files": "^1.1.4",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run lint:tsc",
      "npm run format"
    ]
  }
}
