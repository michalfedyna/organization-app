{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src/",
    "paths": {
      "@components/*": ["components/*"],
      "@developer/*": ["developer/*"],
      "@navigation/*": ["navigation/*"],
      "@theme/*": ["theme/*"],
